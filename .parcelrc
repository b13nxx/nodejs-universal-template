{
  "extends": "@parcel/config-default",
  "transformers": {
    "blob-url:*": [
      "...",
      "@parcel/transformer-inline"
    ],
    "*.xml": [
      "@parcel/transformer-raw"
    ]
  },
  "optimizers": {
    "blob-url:*": [
      "...",
      "@parcel/optimizer-blob-url"
    ]
  },
  "reporters": [
    "...",
    "parcel-reporter-clean-dist"
  ]
}
